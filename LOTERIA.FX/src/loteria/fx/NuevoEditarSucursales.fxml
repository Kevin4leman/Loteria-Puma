<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="299.0" prefWidth="422.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loteria.fx.NuevoEditarSucursalesController">
   <children>
      <Button fx:id="btnAceptar" layoutX="140.0" layoutY="232.0" mnemonicParsing="false" onAction="#aceptar" text="Aceptar" />
      <Button fx:id="btnCancelar" layoutX="221.0" layoutY="232.0" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
      <HBox layoutX="60.0" layoutY="29.0">
         <children>
            <VBox alignment="CENTER_RIGHT" prefHeight="185.0" prefWidth="153.0" spacing="18.0">
               <children>
                  <Label text="Código" />
                  <Label text="RTN" />
                  <Label text="Nombre sucursal" />
                  <Label text="Nombre Representante" />
                  <Label text="Dirección" />
               </children>
               <padding>
                  <Insets right="10.0" />
               </padding>
            </VBox>
            <VBox alignment="CENTER" spacing="10.0">
               <children>
                  <TextField fx:id="txtCodigo" disable="true" />
                  <TextField fx:id="txtRTN" />
                  <TextField fx:id="txtNombreSuc" />
                  <TextField fx:id="txtNombreRepe" />
                  <TextField fx:id="txtDireccion" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
